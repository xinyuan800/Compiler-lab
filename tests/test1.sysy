int i,j=1,k=2;
int main(){
    int a=0;
    int count=0;
    while(a<=0){
        int b;
        a=a-1+b;
        count=count+1;
        while(a<=0&&a>-3){
            count = count * 2;
            a = a - 1;
        }
        if(a<-20){
            break;
        }
        b = b + 1;
    }
    return count;
}